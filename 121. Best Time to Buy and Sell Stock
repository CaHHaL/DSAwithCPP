class Solution {
public:
    int maxProfit(vector<int>& prices) {
     int buy=prices[0];
     int maxp=0;
     for(int i=1;i<prices.size();i++){
        if(prices[i]>buy){
            int cp=prices[i]-buy;
            maxp=max(maxp,cp);
        }
        buy=min(buy,prices[i]);
     }
    return maxp;
    }

};